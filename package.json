    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/speakeasy": "^2.0.10",
    "@types/ssh2": "^1.15.5",
    "@types/ws": "^8.18.1",
